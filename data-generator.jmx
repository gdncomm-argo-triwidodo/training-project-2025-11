    &quot;Docking Station&quot;, &quot;External Drive&quot;, &quot;Network Card&quot;, &quot;Bluetooth Adapter&quot;, &quot;Smart Display&quot;, &quot;VR Headset&quot;, &quot;Game Controller&quot;]

def brands = [&quot;TechPro&quot;, &quot;NovaTech&quot;, &quot;CyberCore&quot;, &quot;DataSync&quot;, &quot;CloudMax&quot;, &quot;ByteForce&quot;, &quot;NetPrime&quot;, &quot;DigiWare&quot;, &quot;SyncMaster&quot;, &quot;CoreTech&quot;,
    &quot;VeloCity&quot;, &quot;ZenithPro&quot;, &quot;ApexTech&quot;, &quot;PrimeWare&quot;, &quot;MaxCore&quot;, &quot;InnoTech&quot;, &quot;FutureTech&quot;, &quot;SmartCore&quot;, &quot;LogiPro&quot;, &quot;HyperTech&quot;,
    &quot;OmniTech&quot;, &quot;QuantumByte&quot;, &quot;NexGen&quot;, &quot;AlphaTech&quot;, &quot;TitanCore&quot;]

def series = [&quot;X1&quot;, &quot;X2&quot;, &quot;X3&quot;, &quot;Pro&quot;, &quot;Max&quot;, &quot;Plus&quot;, &quot;Ultra&quot;, &quot;Lite&quot;, &quot;Air&quot;, &quot;Mini&quot;, &quot;SE&quot;, &quot;GT&quot;, &quot;RS&quot;, &quot;XL&quot;, &quot;S&quot;, &quot;M&quot;, &quot;L&quot;,
    &quot;V1&quot;, &quot;V2&quot;, &quot;V3&quot;, &quot;Gen1&quot;, &quot;Gen2&quot;, &quot;Gen3&quot;, &quot;Mark1&quot;, &quot;Mark2&quot;, &quot;2024&quot;, &quot;3000&quot;, &quot;5000&quot;, &quot;7000&quot;, &quot;9000&quot;]

def descStarts = [
    &quot;Experience unparalleled performance with&quot;,
    &quot;Discover the next generation of&quot;,
    &quot;Unleash your potential with&quot;,
    &quot;Transform your workflow with&quot;,
    &quot;Elevate your experience with&quot;,
    &quot;Achieve more with&quot;,
    &quot;Power your success with&quot;,
