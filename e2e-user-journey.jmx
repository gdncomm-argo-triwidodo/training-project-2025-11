def domains = [&quot;gmail.com&quot;, &quot;yahoo.com&quot;, &quot;outlook.com&quot;, &quot;hotmail.com&quot;, &quot;mail.com&quot;]

def random = new Random()
def threadNum = ctx.getThreadNum()
def loopNum = vars.get(&quot;__jm__TC_01_Register__idx&quot;) ?: &quot;0&quot;
def timestamp = System.currentTimeMillis()

def firstName = firstNames[random.nextInt(firstNames.size())]
def lastName = lastNames[random.nextInt(lastNames.size())]
def domain = domains[random.nextInt(domains.size())]

// Create unique email
def email = &quot;${firstName.toLowerCase()}.${lastName.toLowerCase()}.${threadNum}.${timestamp}@${domain}&quot;
def password = &quot;SecurePass123!&quot;

vars.put(&quot;USER_EMAIL&quot;, email)
vars.put(&quot;USER_PASSWORD&quot;, password)
vars.put(&quot;USER_FIRST_NAME&quot;, firstName)
vars.put(&quot;USER_LAST_NAME&quot;, lastName)

log.info(&quot;Generated user: &quot; + email)
